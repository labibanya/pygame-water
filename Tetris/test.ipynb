    "# 打印一个斐波那契函数\n",
    "def fib(n):\n",
    "    # 打印到 n 的斐波那契数列\n",
    "    a, b = 0, 1\n",
    "    while a < n:\n",
    "        print(a, end=' ')\n",
    "        a, b = b, a + b\n",
    "# 斐波那契数列\n",
    "fib(10)"
   ]
  }
