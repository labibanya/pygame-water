    "# 写一个斐波那契函数\n",
    "def fib(n):\n",
    "    a, b = 0, 1\n",
    "    for i in range(n):\n",
    "        a, b = b, a + b\n",
    "    return a\n",
    "\n",
    "# 斐波那契数列\n",
    "print(fib(3))"
   ]
  }
